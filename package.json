{
  "name": "cz-cl-jira-conventional-changelog",
  "version": "0.0.0",
  "description": "A Commitizen and Commitlint adapter for Jira commit messages.",
  "main": "build/index.js",
  "scripts": {
    "test": "jest",
    "lint": "gts lint && prettier --check .",
    "clean": "gts clean",
    "commit": "npm run compile && cz",
    "compile": "tsc",
    "fix": "gts fix && prettier --write .",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Intregrisist/cz-cl-jira-conventional-changelog.git"
  },
  "keywords": [
    "commitizen",
    "commitlint"
  ],
  "author": "Daniel Flores <dflores009@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Intregrisist/cz-cl-jira-conventional-changelog/issues"
  },
  "homepage": "https://github.com/Intregrisist/cz-cl-jira-conventional-changelog#readme",
  "devDependencies": {
    "@jest/globals": "^29.2.2",
    "@types/inquirer": "^9.0.2",
    "@types/lodash": "^4.14.188",
    "@types/node": "^14.11.2",
    "commitizen": "^4.2.5",
    "gts": "^3.1.1",
    "jest": "^29.2.2",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
